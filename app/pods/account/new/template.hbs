<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <form role="form">
                            <div class="form-group">
                                <label>Nome</label>
                                {{input class="form-control" type="text" value=model.name}}
                            </div>

                            <div class="form-group">
                                <label>CNPJ</label>
                                {{input class="form-control" type="text" value=model.description}}
                            </div>

                            <button type="submit" class="btn btn-default" {{action 'saveAccount' model}}>Salvar</button>
                        </form>
                    </div>
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
